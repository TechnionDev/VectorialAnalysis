\begin_layout Standard
Need to calculate 
\begin_inset Formula $\int_{C}x^{2}ydx-y^{2}xdy$
\end_inset

 with 
\begin_inset Formula $C$
\end_inset

 is the upper half circle 
\begin_inset Formula $x^{2}+y^{2}=a^{2}$
\end_inset

 counterclockwise.
\end_layout

\begin_layout Proof
Let's look at the parameterization of 
\begin_inset Formula $r\left(t\right)=\left(a\cos\left(t\right),a\sin\left(t\right),0\right)$
\end_inset

 for 
\begin_inset Formula $0\leq t\leq\pi$
\end_inset

.
\end_layout

\begin_layout Proof
So 
\begin_inset Formula $r'\left(t\right)=\left(-a\sin t,a\cos t,0\right)$
\end_inset


\begin_inset Formula 
\[
\int_{C}x^{2}ydx-y^{2}xdy=\int_{t=0}^{\pi}\left(a\cos t\right)^{2}\left(a\sin t\right)\und{\left(-a\sin t\right)}{\text{r'\left(t\right)_{x}}}-\left(a\sin t\right)^{2}\left(a\cos t\right)\und{\left(a\cos t\right)}{\text{\ensuremath{r'\left(t\right)_{y}}}}dt=
\]

\end_inset


\begin_inset Formula 
\[
=\int_{t=0}^{\pi}a^{4}\cos^{2}t\sin^{2}t\cdot\left(-1-1\right)dt=-\frac{1}{2}a^{4}\int_{t=0}^{\pi}\left(2\cos t\sin t\right)^{2}dt=-\frac{1}{2}a^{4}\int_{t=0}^{\pi}\left(\sin2t\right)^{2}dt=
\]

\end_inset


\begin_inset Formula 
\[
=-\frac{1}{2}a^{4}\int_{t=0}^{\pi}\frac{1}{2}-\cos4t\,dt=-\frac{1}{2}a^{4}\left(\frac{\pi}{2}-\und 0{\text{\ensuremath{\int_{t=0}^{\pi}\cos4t\,dt}}}\right)=-\frac{1}{4}\pi a^{4}
\]

\end_inset


\end_layout

